{
  "name": "mlnomadpy-portfolio",
  "version": "2.0.0",
  "description": "Personal portfolio and research website for Taha Bouhsine (MLNomadpy)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .vue,.js",
    "lint:fix": "eslint src --ext .vue,.js --fix",
    "format": "prettier --write \"src/**/*.{js,vue,css}\""
  },
  "keywords": [
    "portfolio",
    "vue",
    "machine-learning",
    "research"
  ],
  "author": "Taha Bouhsine",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "happy-dom": "^19.0.2",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.9",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.2.4"
  }
}
